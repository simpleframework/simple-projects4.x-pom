  <!--仓库配置 -->
  <repositories>
    <repository>
      <id>public</id>
      <url>http://maven.simpleframework.net/content/groups/public</url>
      <snapshots>
        <!-- 资源库加载快照版本 -->
        <enabled>true</enabled>
        <!-- 允许更新同步快照 -->
        <updatePolicy>always</updatePolicy>
      </snapshots>
      <releases>
        <!-- 资源库加载稳定版本 -->
        <enabled>true</enabled>
      </releases>
    </repository>
  </repositories>

